   },
   "dependencies": {
      "express": "^4.17.1",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.13",
      "sqlite3": "^5.0.0",
      "tsconfig-paths": "^3.11.0",
      "typeorm": "0.2.37"
   },
   "devDependencies": {
      "@types/express": "^4.17.7",
      "@types/node": "^14.6.0",
      "nodemon": "^2.0.4",
      "typescript": "3.9.7",
      "ts-node": "8.10.2"
   }
